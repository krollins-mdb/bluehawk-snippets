{
	"Bluehawk: start snippet": {
		"prefix": "bss",
		"body": [
			"$LINE_COMMENT :snippet-start: ${1:snippet-name}"
		],
		"description": "Start Bluehawk snippet"
	},
	"Bluehawk: end snippet": {
		"prefix": "bse",
		"body": [
			"$LINE_COMMENT :snippet-end:$1"
		],
		"description": "End Bluehawk snippet"
	},
	"Bluehawk: full snippet": {
		"prefix": "bsf",
		"body": [
			"$LINE_COMMENT :snippet-start: ${1:snippet-name}",
			"$2",
			"$LINE_COMMENT :snippet-end:"
		],
		"description": "Full Bluehawk snippet"
	},
	"Bluehawk: start replace": {
		"prefix": "brs",
		"body": [
			"$LINE_COMMENT :replace-start: {",
			"$LINE_COMMENT    \"terms\": {",
			"$LINE_COMMENT       \"${1:TermToReplace}\": \"${2:ReplaceWith}\"",
			"$LINE_COMMENT    }",
			"$LINE_COMMENT }"
		],
		"description": "Start Bluehawk snippet"
	},
	"Bluehawk: end replace": {
		"prefix": "bre",
		"body": [
			"$LINE_COMMENT :replace-end:$1"
		],
		"description": "End Bluehawk snippet"
	},
	"Bluehawk: full replace": {
		"prefix": "brf",
		"body": [
			"$LINE_COMMENT :replace-start: {",
			"$LINE_COMMENT    \"terms\": {",
			"$LINE_COMMENT       \"${1:TermToReplace}\": \"${2:ReplaceWith}\"",
			"$LINE_COMMENT    }",
			"$LINE_COMMENT }",
			"$3",
			"$LINE_COMMENT :replace-end:"
		],
		"description": "Full Bluehawk snippet"
	},
